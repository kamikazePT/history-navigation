{
  "name": "history-navigation",
  "version": "0.0.0",
  "description": "History Navigation Utils",
  "workspaces": [
    "packages/*"
  ],
  "repository": "https://github.com/kamikazePT/history-navigation",
  "author": "Afonso Santos aka KamikazePT",
  "license": "MIT",
  "private": true,
  "scripts": {
    "add:root": "yarn add -W",
    "add:package": "lerna add --scope",
    "preinstall": "scripts/preinstall.js",
    "bootstrap": "lerna bootstrap",
    "bootstrap:integrity": "yarn check--integrity && lerna exec -- yarn check --integrity",
    "prebuild": "lerna exec --parallel -- rm -rf dist",
    "build:debug": "yarn prebuild && cross-env NODE_ENV=development lerna exec --skip-git --skip-npm --parallel -- rollup -c=../../rollup.config.js",
    "build": "cross-env NODE_ENV=production lerna exec --skip-git --skip-npm --parallel -- rollup -c=../../rollup.config.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint '**/*.{js,jsx}'",
    "format": "prettier-eslint '**/*.{js,jsx}' --write",
    "doc": "cross-env NODE_ENV=development lerna exec --parallel -- jsdoc-md -s '**/*.{js,jsx}' -m README.md -t API"
  },
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">=1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-runner-eslint": "^0.7.3",
    "@kamikazept/jsdoc-md": "^1.0.0",
    "lerna": "^3.15.0",
    "lint-staged": "^8.1.7",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "resolve-url-loader": "^3.1.0",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-multi-input": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.0.0"
  }
}
